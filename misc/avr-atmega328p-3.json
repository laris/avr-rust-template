{
  "arch": "avr",
  "cpu": "atmega328p",
  "data-layout": "e-P1-p:16:8-i8:8-i16:8-i32:8-i64:8-f32:8-f64:8-n8-a:8",
  "exe-suffix": ".elf",
  "executables": true,
  "is-builtin": false,
  "linker": "avr-gcc",
  "linker-flavor": "gcc",
  "linker-is-gnu": true,
  "llvm-target": "avr-unknown-unknown",
  "max-atomic-width": 8,
  "eh-frame-header": false,
  "os": "none",
  "target-c-int-width": "16",
  "target-endian": "little",
  "target-pointer-width": "16",
  "no-compiler-rt": false,
  "pre-link-args": {
    "gcc": [
      "-mmcu=atmega328p",
      "-nostartfiles"
    ]
  },
  "post-link-args": {
    "gcc": [
      "-Wl,--entry=main"
    ]
  }
}
